s/studio/config","docs/9.studio/5.config",{"title":233,"path":234,"stem":235},"本地调试","/docs/studio/debug","docs/9.studio/6.debug",{"id":237,"title":238,"body":239,"description":240,"extension":241,"meta":242,"navigation":243,"path":244,"seo":245,"stem":246,"__hash__":247},"landing/templates.yml","模板",{"title":238,"description":240},"通过我们量身定制的模板快速启动您的下一个网站。点击即可部署。实时编辑并分享实时预览网址。","yml",{},true,"/templates",{"title":238,"description":240},"templates","nhyuLXeGlpbMLwqC7gcsvx4QOvxnmdoZJT7btPGZ_U0",[249,356,441,725,799,840,915],{"id":250,"title":251,"baseDir":252,"body":253,"branch":339,"category":340,"demo":341,"description":342,"draft":50,"extension":343,"image1":344,"image2":345,"image3":346,"licenseType":347,"mainScreen":348,"meta":349,"name":350,"navigation":243,"owner":351,"path":352,"seo":353,"slug":350,"stem":354,"subtitle":331,"__hash__":355},"templates/templates/canvas.md","画布",".",{"type":254,"value":255,"toc":336},"minimark",[256],[257,258,259,263,328],"template-core",{},[260,261,262],"p",{},"Canvas 作品集是一个完全可定制的国际化作品集模板，使用 Nuxt 和 Nuxt UI 构建，旨在帮助您轻松展示作品、推荐语和关键信息。该模板集成了 Nuxt Studio，提供流畅的编辑体验，同时利用 Nuxt Content 进行内容管理。Canvas 作品集在性能、灵活性和响应式设计方面表现出色，为开发者和创意人员提供了坚实的基础。",[264,265,266,274,280,286,292,298,304,310,316,322],"ul",{},[267,268,269,273],"li",{},[270,271,272],"strong",{},"现代组件与布局"," – 包含内置组件。",[267,275,276,279],{},[270,277,278],{},"Nuxt UI v3"," – 使用预制的可定制 UI 组件。",[267,281,282,285],{},[270,283,284],{},"NuxtHub 支持"," - 几秒钟内部署到 NuxtHub。",[267,287,288,291],{},[270,289,290],{},"Tailwind CSS"," – 美观且响应式的设计系统。",[267,293,294,297],{},[270,295,296],{},"可用的联系表单"," – 集成 Resend，轻松处理邮件。",[267,299,300,303],{},[270,301,302],{},"多语言支持"," – 由 Nuxt i18n 驱动。",[267,305,306,309],{},[270,307,308],{},"SEO 友好"," – 支持 Open Graph 图片（Nuxt OG Image）和 Nuxt Robots，实现自动生成 robots.txt。",[267,311,312,315],{},[270,313,314],{},"良好实践"," – 自动生成网站地图，优化图片（Nuxt Image），以及 ESLint（Nuxt 配置采用 Flat 配置）。",[267,317,318,321],{},[270,319,320],{},"完全响应式"," – 兼容所有现代浏览器和设备。",[267,323,324,327],{},[270,325,326],{},"极简且专业的设计"," – 简洁、优雅且易于定制。",[329,330,332],"template",{"v-slot:right":331},"",[333,334],"template-features",{":features":335},"[{\"label\":\"Nuxt 架构\",\"content\":\"充分利用 Nuxt 3 及其模块生态系统的强大功能。\"},{\"label\":\"支持 Nuxt Studio\",\"content\":\"在 Nuxt Studio 中实时预览并编辑主题内容和外观。\"},{\"label\":\"Vue 组件\",\"content\":\"在内容中插入内置组件（或自己的组件）。\"},{\"label\":\"书写 Markdown\",\"content\":\"享受 Markdown 的简便与轻松，探索 MDC 语法。\"},{\"label\":\"随处部署\",\"content\":\"通过 Studio 一键部署，或在 Vercel、Netlify 零配置部署。可选择静态生成、按需渲染（Node），或 CloudFlare Workers 的边缘渲染。\"},{\"label\":\"可扩展\",\"content\":\"自定义整体设计，或通过插槽添加组件——您可以打造属于自己的 Content-Wind。\"}]",{"title":331,"searchDepth":337,"depth":337,"links":338},2,[],"main","作品集","https://canvas.hrcd.fr/","这是一个支持国际化的完全可定制的作品集模板。","md","/templates/canvas-portfolio-1.png","/templates/canvas-portfolio-2.png","/templates/canvas-portfolio-3.png","free","/templates/canvas.jpg",{},"canvas","HugoRCD","/templates/canvas",{"title":251,"description":342},"templates/canvas","ypy_KMy7JfLzGwAo8Lb0_Wn_hHLWgm0udXIPiv7XzjI",{"id":357,"title":358,"baseDir":252,"body":359,"branch":339,"category":427,"demo":428,"description":429,"draft":50,"extension":343,"image1":430,"image2":431,"image3":432,"licenseType":433,"mainScreen":434,"meta":435,"name":427,"navigation":243,"owner":433,"path":437,"seo":438,"slug":427,"stem":439,"subtitle":331,"__hash__":440},"templates/templates/docs.md","文档",{"type":254,"value":360,"toc":425},[361],[257,362,363,379,382,420],{},[260,364,365,372,373,378],{},[366,367,371],"a",{"href":368,"rel":369},"https://ui.nuxt.com/pro",[370],"nofollow","Nuxt UI Pro"," 是一组高级组件，作为 ",[366,374,377],{"href":375,"rel":376},"https://ui.nuxt.com",[370],"Nuxt UI"," 的扩展，旨在帮助您在几分钟内创建漂亮且响应式的 Nuxt 应用。",[260,380,381],{},"Nuxt UI 团队致力于提供最佳的集成和定制体验，同时 Studio 团队提供与 Nuxt Studio 的完全兼容性。",[264,383,384,390,396,402,408,414],{},[267,385,386,389],{},[270,387,388],{},"完全可定制","：可通过您的应用配置更改任意组件的样式，或通过 ui 属性进行专门定制。",[267,391,392,395],{},[270,393,394],{},"轻松编写 Markdown","：Nuxt UI Pro 重写了 Nuxt Content 的 prose 组件，使其更出色，还新增了 Callout、CodeGroup、Field 等组件。",[267,397,398,401],{},[270,399,400],{},"精美的排版样式","：预配置了 Tailwind CSS 排版插件，样式与 Nuxt UI 组件和颜色匹配。",[267,403,404,407],{},[270,405,406],{},"开箱即用的全文搜索","：Nuxt UI Pro 配备了现成的命令面板组件，无需再配置 Algolia DocSearch。",[267,409,410,413],{},[270,411,412],{},"全面插槽支持","：每个组件都利用 Vue 的插槽功能，给予您构建任意内容的灵活性。",[267,415,416,419],{},[270,417,418],{},"响应式设计","：Nuxt UI Pro 组件旨在构建您的内容结构，天生响应式，适应各种屏幕尺寸。",[329,421,422],{"v-slot:right":331},[333,423],{":features":424},"[{\"label\":\"Nuxt 3\",\"content\":\"由 Nuxt 3 驱动，带来最佳性能和 SEO。\"},{\"label\":\"Markdown\",\"content\":\"借助 Nuxt Content 使用 MDC 编写页面。\"},{\"label\":\"Nuxt UI\",\"content\":\"提供大量完全可定制的组件。\"},{\"label\":\"TypeScript\",\"content\":\"享受完备的类型化开发体验。\"},{\"label\":\"Nuxt Studio\",\"content\":\"支持 Nuxt Studio，实现快速更新和预览。\"},{\"label\":\"搜索\",\"content\":\"由 Fuse.js 驱动的全文搜索模态框。\"}]",{"title":331,"searchDepth":337,"depth":337,"links":426},[],"docs","https://v3.docs-template-3erl.pages.dev/","使用此模板，几秒钟内即可创建您的文档。","/templates/docs-1.webp","/templates/docs-2.webp","/templates/docs-3.webp","nuxt-ui-pro","/templates/docs.jpg",{"createdAt":436},"2023-11-15T17:41:03.087Z","/templates/docs",{"title":358,"description":429},"templates/docs","I58b2zhqqk27euQdxvxvpkpxp0YU_iyKNBu0_pNO2Qg",{"id":442,"title":443,"baseDir":444,"body":445,"branch":339,"category":427,"demo":656,"description":715,"draft":50,"extension":343,"image1":716,"image2":331,"image3":331,"licenseType":433,"mainScreen":717,"meta":718,"name":719,"navigation":243,"owner":720,"path":721,"seo":722,"slug":719,"stem":723,"subtitle":331,"__hash__":724},"templates/templates/docus.md","Docus",".starters/default",{"type":254,"value":446,"toc":706},[447,702],[257,448,449,455,458,463,520,524,575,581,585,593,597,600,648,651,659,662,665,679,686,690,697],{},[450,451,452],"blockquote",{},[260,453,454],{},"一个美观、极简的 Docus 文档创建入门模板",[260,456,457],{},"这是默认的 Docus 入门模板，提供了使用 Markdown 和 Vue 组件构建漂亮文档网站所需的一切。",[459,460,462],"h2",{"id":461},"特性","✨ 特性",[264,464,465,472,479,486