itle":228,"path":229,"stem":230},"内容编辑器","/docs/studio/content","docs/9.studio/5.content",{"title":232,"path":233,"stem":234},"媒体库","/docs/studio/medias","docs/9.studio/6.medias",{"title":236,"path":237,"stem":238},"路线图","/docs/studio/roadmap","docs/9.studio/7.roadmap",{"title":240,"path":241,"stem":242},"同步","/docs/studio/advanced","docs/9.studio/8.advanced",{"page":244,"surround":919},{"id":245,"title":246,"authors":247,"body":253,"categories":907,"category":908,"date":909,"description":910,"draft":50,"extension":911,"image":912,"meta":914,"navigation":446,"path":915,"seo":916,"stem":917,"__hash__":918},"posts/changelog/frontmatter-form.md","可视化 Front-matter 编辑",[248],{"name":249,"avatar":250,"to":252},"Baptiste Leproux",{"src":251},"https://avatars.githubusercontent.com/u/7290030?v=4","https://x.com/_larbish",{"type":254,"value":255,"toc":896},"minimark",[256,278,283,291,297,301,309,314,317,320,323,327,335,339,349,352,356,364,367,371,374,379,386,390,393,742,746,755,758,761,888,892],[257,258,259,260,267,268,273,274,277],"warning",{},"本文发布于 2025 年 1 月 6 日 ",[261,262,266],"a",{"href":263,"rel":264},"https://github.com/nuxt/content",[265],"nofollow","Content"," 和 ",[261,269,272],{"href":270,"rel":271},"https://github.com/nuxtlabs/studio-module",[265],"Studio"," 模块合并之前。因此，本文可能存在一些不一致之处。Studio 模块现已弃用，作为 Content 模块的可选功能提供。请参阅",[261,275,276],{"href":6},"这份指南","了解如何启用。",[279,280,282],"h2",{"id":281},"可视化-front-matter-编辑","可视化 Front-Matter 编辑",[284,285,286,287,290],"p",{},"现在您无需编写 ",[288,289,62],"code",{}," 语法即可编辑 markdown front-matter。Nuxt Studio 会自动生成用户友好的表单，简化元数据编辑。",[292,293],"video",{":autoplay":294,":controls":294,":loop":294,"poster":295,"src":296},"true","https://res.cloudinary.com/nuxt/video/upload/v1729157955/frontmatterform2_rmh58v.jpg","https://res.cloudinary.com/nuxt/video/upload/v1729157955/frontmatterform2_rmh58v.mp4",[279,298,300],{"id":299},"什么是-front-matter","什么是 Front-matter？",[284,302,303,304,308],{},"Front-matter 是基于 Markdown 的 CMS 中用于为页面提供元数据（如描述、标题等）的约定。在 ",[261,305,307],{"href":306},"/docs/files/markdown#front-matter","Nuxt Content"," 中，front-matter 使用的是 YAML 语法。",[310,311,313],"callout",{"icon":312,"to":306},"i-ph-info","关于 front-matter 语法的详细信息，请参阅 Nuxt Content 文档。",[279,315,316],{"id":316},"我们非技术编辑器的最后一环",[284,318,319],{},"Nuxt Studio 从发布编辑器起就以非技术用户为设计核心。我们的目标是让每个人都能轻松编辑 markdown 和内容。",[284,321,322],{},"自动生成 front-matter 表单是下一步的合理进展。通过摆脱 YAML 语法的复杂性，简化了非开发者的编辑流程，提供动态输入选项，如图片选择器、日期选择器、布尔切换等。此增强使我们实现了完全可视化且用户友好的内容管理体验。",[279,324,326],{"id":325},"扩展至所有-yaml-和-json-文件","扩展至所有 YAML 和 JSON 文件",[284,328,329,330,267,332,334],{},"不久之后，表单生成功能将扩展至您在 Nuxt Studio 中编辑的所有 ",[288,331,62],{},[288,333,66],{}," 文件，让结构化数据的操作更为轻松便捷。",[279,336,338],{"id":337},"展望-nuxt-content-v3","展望 Nuxt Content v3",[310,340,342,343,348],{"icon":341},"i-ph-lightbulb","本节内容仅为 ",[261,344,347],{"href":345,"rel":346},"https://github.com/nuxt/content/tree/v3",[265],"Nuxt Content v3"," 的预告，我们将很快发布更详尽的博文。",[284,350,351],{},"我们正积极开发下一重大版本 Nuxt Content，这将带来显著的性能提升和新功能，进一步提升内容管理体验。",[353,354,355],"h3",{"id":355},"性能优化",[284,357,358,359,363],{},"Nuxt Content v2 的一大挑战是需要较大的包体积来存储所有内容文件，这在向边缘平台（如 ",[261,360,132],{"href":361,"rel":362},"https://hub.nuxt.com/",[265],"）部署时造成了问题。",[284,365,366],{},"为解决此问题，Nuxt Content v3 在生产环境中不再使用基于文件的存储方式，而是采用 SQL 数据库系统。此变更对用户透明。我们为开发模式、静态生成、服务器渲染及通过 NuxtHub 的边缘部署提供零配置支持。",[353,368,370],{"id":369},"引入-collections集合","引入 Collections（集合）",[284,372,373],{},"Collections 是 Nuxt Content 项目中相关内容项的分组，帮助更高效地组织和管理大型数据集。",[375,376,378],"h4",{"id":377},"定义-collections","定义 Collections",[284,380,381,382,385],{},"您可以在 ",[288,383,384],{},"content.config.ts"," 文件中定义 collections，Nuxt Content 将用它配置数据库结构、类型工具及用于查找、解析和查询内容的方法。",[375,387,389],{"id":388},"collections-方案schema","Collections 方案（Schema）",[284,391,392],{},"方案可确保 collections 内部的一致性，并改善 TypeScript 类型定义，增强与 Nuxt Content 工具的集成。",[394,395,400],"pre",{"className":396,"code":397,"filename":384,"language":398,"meta":399,"style":399},"language-ts shiki shiki-themes material-theme-lighter material-theme material-theme-palenight","import { defineCollection, z } from '@nuxt/content'\n\n// 导出 collections\nexport const collections = {\n  // 使用 `defineCollection` 工具定义 collection\n  posts: defineCollection({\n    // 指定该 collection 中内容的类型\n    type: 'page',\n    // 加载匹配此模式的所有文件\n    source: 'blog/**/*.md',\n    // 为该 collection 定义自定义 schema\n    schema: z.object({\n      date: z.date(),\n      image: z.object({\n        src: z.string(),\n        alt: z.string()\n      }),\n      badge: z.object({\n        label: z.string(),\n        color: z.string()\n      })\n    })\n  }),\n}\n","ts","",[288,401,402,441,448,455,474,480,499,505,524,530,547,553,573,593,611,630,647,658,676,694,710,718,726,736],{"__ignoreMap":399},[403,404,407,411,415,419,422,425,428,431,434,438],"span",{"class":405,"line":406},"line",1,[403,408,410],{"class":409},"s7zQu","import",[403,412,414],{"class":413},"sMK4o"," {",[403,416,418],{"class":417},"sTEyZ"," defineCollection",[403,420,421],{"class":413},",",[403,423,424],{"class":417}," z",[403,426,427],{"class":413}," }",[403,429,430],{"class":409}," from",[403,432,433],{"class":413}," '",[403,435,437],{"class":436},"sfazB","@nuxt/content",[403,439,440],{"class":413},"'\n",[403,442,444],{"class":405,"line":443},2,[403,445,447],{"emptyLinePlaceholder":446},true,"\n",[403,449,451],{"class":405,"line":450},3,[403,452,454],{"class":453},"sHwdD","// 导出 collections\n",[403,456,458,461,465,468,471],{"class":405,"line":457},4,[403,459,460],{"class":409},"export",[403,462,464],{"class":463},"spNyl"," const",[403,466,467],{"class":417}," collections ",[403,469,470],{"class":413},"=",[403,472,473],{"class":413}," {\n",[403,475,477],{"class":405,"line":476},5,[403,478,479],{"class":453},"  // 使用 `defineCollection` 工具定义 collection\n",[403,481,483,487,490,493,496],{"class":405,"line":482},6,[403,484,486],{"class":485},"swJcz","  posts",[403,488,489],{"class":413},":",[403,491,418],{"class":492},"s2Zo4",[403,494,495],{"class":417},"(",[403,497,498],{"class":413}